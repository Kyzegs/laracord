{
    "name": "kyzegs/laracord",
    "description": "Discord API wrapper",
    "keywords": ["discord"],
    "authors": [
        {
            "name": "Sebastiaan \"Kyzegs\" Zegers",
            "email": "kyzegs@gmail.com"
        }
    ],
    "homepage": "https://github.com/Kyzegs/laracord",
    "autoload": {
        "psr-4": {
            "Kyzegs\\Laracord\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kyzegs\\Laracord\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/cache": "^10.16",
        "illuminate/collections": "^10.16",
        "illuminate/support": "^10.16",
        "kyzegs/model": "^0.1.0",
        "laravel/socialite": "^5.8"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpdoc-parser": "^1.23"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kyzegs\\Laracord\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
