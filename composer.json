{
    "name": "kyzegs/laracord",
    "description": "Discord API wrapper",
    "keywords": ["discord"],
    "authors": [
        {
            "name": "Sebastiaan \"Kyzegs\" Zegers",
            "email": "kyzegs@gmail.com"
        }
    ],
    "homepage": "https://github.com/Kyzegs/laracord",
    "autoload": {
        "psr-4": {
            "Kyzegs\\Laracord\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kyzegs\\Laracord\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/socialite": "^5.5",
        "jenssegers/model": "^1.5",
        "doctrine/dbal": "^3.4"
    },
    "require-dev": {
        "orchestra/testbench": "^7.4",
        "phpunit/phpunit": "^9.5",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kyzegs\\Laracord\\Providers\\LaracordServiceProvider"
            ],
            "aliases": {
                "ApplicationCommand": "Kyzegs\\Laracord\\Facades\\ApplicationCommand",
                "ApplicationCommandPermissions": "Kyzegs\\Laracord\\Facades\\ApplicationCommandPermissions",
                "Http": "Kyzegs\\Laracord\\Facades\\Http"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
