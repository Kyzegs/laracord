{
    "name": "kyzegs/laracord",
    "description": "Discord API wrapper",
    "type": "library",
    "authors": [
        {
            "name": "Sebastiaan Zegers",
            "email": "kyzegs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kyzegs\\Laracord\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/socialite": "^5.2",
        "jenssegers/model": "^1.4"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kyzegs\\Laracord\\LaracordServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
